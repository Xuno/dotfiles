
urxvt.font: xft:Anonymous Pro:style=Regular:pixelsize=18,xft:wenquanyi bitmap song:medium:pixelsize=16
urxvt.boldFont: xft:Anonymous Pro:style=Bold:pixelsize=18,xft:wenquanyi bitmap song:bold:pixelsize=16
urxvt.italicFont: xft:Anonymous Pro:style=Italic:pixelsize=18,xft:wenquanyi bitmap song:medium:pixelsize=16
urxvt.boldItalicFont: xft:Anonymous Pro:style=Bold Italic:pixelsize=18,xft:wenquanyi bitmap song:bold:pixelsize=16

urxvt.perl-ext-common:  default,tabbed,fullscreen,keyboard-select,clipboard

urxvt.keysym.S-Escape: perl:keyboard-select:activate
urxvt.keysym.S-F11: perl:fullscreen:switch

urxvt.keysym.C-A-c: perl:clipboard:copy
urxvt.keysym.C-A-v: perl:clipboard:paste

urxvt.scrollstyle: plain

*background: #000000
*foreground: #ffffff

*color0: #000000
*color1: #ee6363
*color2: #73ba3e
*color3: #baa33e
*color4: #3e73ba
*color5: #ac3885
*color6: #3ebaba
*color7: #eeeeee
*color8: #333333
*color9: #cc4444
*color10: #4d8728
*color11: #877d28
*color12: #284d87
*color13: #812973
*color14: #288787
*color15: #aaaaaa

urxvt.inputMethod: ibus
urxvt.termName: xterm

Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.rgba: rgb

urxvt.transparent:true
urxvt.tintColor:white
urxvt.shading:20

urxvt.geometry: 96x32

urxvt.saveLines: 65536
