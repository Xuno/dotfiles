
urxvt.font: xft:Monaco for Powerline:regular:size=11
urxvt.boldFont: xft:Monaco for Powerline:bold:size=11
urxvt.italicFont: xft:Monaco for Powerline:italic:size=11
urxvt.boldItalicFont: xft:Monaco for Powerline:bold:italic:size=11

urxvt.perl-ext-common:  default,tabbed,fullscreen,keyboard-select,clipboard

urxvt.copyCommand:  xclip -i -selection clipboard
urxvt.pasteCommand: xclip -o -selection clipboard

urxvt.keysym.S-Escape: perl:keyboard-select:activate
urxvt.keysym.S-F11: perl:fullscreen:switch

urxvt.keysym.C-A-c: perl:clipboard:copy
urxvt.keysym.C-A-v: perl:clipboard:paste

urxvt.scrollstyle: plain

*background: #000000
*foreground: #ffffff

*color0: #000000
*color1: #ee6363
*color2: #73ba3e
*color3: #baa33e
*color4: #3e73ba
*color5: #ac3885
*color6: #3ebaba
*color7: #eeeeee
*color8: #333333
*color9: #cc4444
*color10: #4d8728
*color11: #877d28
*color12: #284d87
*color13: #812973
*color14: #288787
*color15: #aaaaaa

urxvt.inputMethod: ibus

Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintlight
Xft.rgba: rgb

urxvt.depth: 32
urxvt.background: [80]black

urxvt.tintColor: white
urxvt.shading: 30

urxvt.geometry: 96x32

urxvt.saveLines: 65536
